# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# OpenAI Configuration (for AI classification)
OPENAI_API_KEY=your-openai-api-key
OPENAI_ORG_ID=your-openai-org-id

# RevenueCat Configuration (for premium features)
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=your-revenuecat-ios-key
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=your-revenuecat-android-key

# Analytics Configuration
EXPO_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token

# App Configuration
EXPO_PUBLIC_APP_ENVIRONMENT=development
EXPO_PUBLIC_APP_VERSION=0.1.0

# Social Login Configuration
EXPO_PUBLIC_GOOGLE_CLIENT_ID_IOS=your-google-ios-client-id
EXPO_PUBLIC_GOOGLE_CLIENT_ID_ANDROID=your-google-android-client-id
EXPO_PUBLIC_APPLE_CLIENT_ID=your-apple-client-id

# Development Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# API Endpoints
EXPO_PUBLIC_API_BASE_URL=https://api.specialtracker.app
EXPO_PUBLIC_HEALTH_API_ENDPOINT=https://health.specialtracker.app

# Feature Flags
EXPO_PUBLIC_ENABLE_AI_CLASSIFICATION=true
EXPO_PUBLIC_ENABLE_SOCIAL_FEATURES=true
EXPO_PUBLIC_ENABLE_PREMIUM_FEATURES=true
EXPO_PUBLIC_ENABLE_HEALTH_INTEGRATION=true

# Push Notifications
EXPO_PUBLIC_PUSH_NOTIFICATIONS_ENABLED=true

# Sentry Configuration (for error tracking)
SENTRY_DSN=your-sentry-dsn
SENTRY_ORG=your-sentry-org
SENTRY_PROJECT=your-sentry-project 